// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Movies {
  id           Int      @id @default(autoincrement())
  title        String
  directorName String
  company      String
  actors       Actors[]
}

model Actors {
  id   Int    @id @default(autoincrement())
  name String
  //Missing one to many relationship here
  Movies   Movies? @relation(fields: [moviesId], references: [id])
  moviesId Int?
}
